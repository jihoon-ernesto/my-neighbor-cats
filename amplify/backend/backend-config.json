{
  "api": {
    "CatsApi": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "CatsBackend"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "auth": {
    "MyCatsAuth": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL"
        ],
        "socialProviders": [],
        "usernameAttributes": [],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "CatsBackend": {
      "build": true,
      "dependsOn": [],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "lambdaForThumbnail": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "BucketName"
          ],
          "category": "storage",
          "resourceName": "CatsBucket"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "storage": {
    "CatsBucket": {
      "dependsOn": [],
      "providerPlugin": "awscloudformation",
      "service": "S3"
    },
    "CatsDB": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}